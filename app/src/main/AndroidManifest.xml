<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.coursal2">

  <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
  <uses-permission android:name="android.permission.INTERNET" />

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".sharedpreferences.SharedPreferencesActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name=".webservices.MyWebservicesActivity">
    </activity>
    <activity android:name=".recyclerview.MyRecyclerActivity">
    </activity>
    <activity android:name=".qualifiers.QualifiersActivity">
    </activity>
    <activity android:name=".fragments.exercise.ActivityA">
    </activity>
    <activity android:name=".fragments.FirstFragmentActivity">
    </activity>
    <activity android:name=".navigation_exercise.WeightActivity">
    </activity>
    <activity android:name=".navigation_exercise.NameActivity">
    </activity>
    <activity android:name=".NavigationDestinationActivity">
    </activity>
    <activity android:name=".NavigationBaseActivity">
    </activity>
    <activity
        android:name=".LifeCycleActivity"
        android:configChanges="orientation"
        android:screenOrientation="portrait">
    </activity>
    <activity android:name=".LogicExerciseActivity">
    </activity>
    <activity android:name=".MainActivity">
    </activity>
  </application>

</manifest>